<?xml version="1.0" encoding="UTF-8"?>
<config>
	<modules>
		<Intersales_SocialSharePrivacy>
			 <version>1.2.0</version>
		</Intersales_SocialSharePrivacy>
	</modules>

	<adminhtml>
		<translate>
			<modules>
				<Intersales_SocialSharePrivacy>
					<files>
						<default>Intersales_SocialSharePrivacy.csv</default>
					</files>
				</Intersales_SocialSharePrivacy>
			</modules>
		</translate>
	</adminhtml>

    <frontend>
        <translate>
            <modules>
                <Intersales_SocialSharePrivacy>
                    <files>
                        <default>Intersales_SocialSharePrivacy.csv</default>
                    </files>
                </Intersales_SocialSharePrivacy>
            </modules>
        </translate>
        <layout>
            <updates>
                <socialshareprivacy>
                    <file>intersales_socialshareprivacy.xml</file>
                </socialshareprivacy>
            </updates>
        </layout>
        <events>
            <core_block_abstract_to_html_before>
                <observers>
                    <socialshareprivacy>
                        <type>model</type>
                        <class>socialshareprivacy/observer</class>
                        <method>addJs</method>
                    </socialshareprivacy>
                </observers>
            </core_block_abstract_to_html_before>
        </events>
    </frontend>

    <global>
        <blocks>
            <socialshareprivacy>
                <class>Intersales_SocialSharePrivacy_Block</class>
            </socialshareprivacy>
        </blocks>

        <helpers>
            <socialshareprivacy>
                <class>Intersales_SocialSharePrivacy_Helper</class>
            </socialshareprivacy>
        </helpers>

        <models>
            <socialshareprivacy>
                <class>Intersales_SocialSharePrivacy_Model</class>
            </socialshareprivacy>
        </models>
    </global>
</config>
